#include<stdio.h>
int main()
{
	int n,i,j=0;
	do{
	scanf("%d",&n);
	}
	while(n>=100);
	int x[n];
	int sum1=0,sum2=0;
	for(i=1;i<=n;++i)
	{
		scanf("%d",&x[i]);
  		if(x[i]<0)
		{
			++j;
			sum1+=x[i];
		}
		if(x[i]>0)
		{
			sum2+=x[i];
		}
	}
	if(sum1!=0) printf("%f %f",sum1/j*1.0,sum2*1.0);
	if(sum1==0) printf("%f %f",0,sum2*1.0);
	
	return 0;
	
}
